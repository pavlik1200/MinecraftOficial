$((function(){0==getCookie("access_token").length?(jQuery(".nav > #app-profile").remove(),jQuery(".nav > .profile-widget__login-link").removeClass("hide"),jQuery(".menusignin").removeClass("hidden")):getCookie("access_token").length>0&&(jQuery(".nav > .profile-widget__login-link").remove(),jQuery(".nav > #app-profile").find("#profiledd1").text(getCookie("session_username")||""),jQuery(".nav > #app-profile").removeClass("hide"),jQuery(".menusignin").remove(),jQuery(".menusigndetails").removeClass("hidden"),jQuery(".menusigndetails").find("#profilename").text(getCookie("session_username")||""))}));